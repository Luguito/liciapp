{
  "name": "@addi/ui",
  "version": "0.1.0",
  "private": true,
  "peerDependencies": {
    "file-loader": "5.0.2",
    "prop-types": "^15.7.2",
    "react": "*",
    "react-dom": "*",
    "styled-components": "^5.2.1"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@types/react-dom": "16.8.5",
    "@types/react-input-mask": "^3.0.0",
    "cleave.js": "^1.6.0",
    "copyfiles": "2.4.1",
    "i18next": "^19.8.4",
    "i18next-http-backend": "^1.0.21",
    "lodash": "4.17.21",
    "react-currency-input-field": "3.3.0",
    "react-device-detect": "^1.17.0",
    "react-i18next": "^11.8.4",
    "react-input-mask": "^2.0.4",
    "react-transition-group": "^4.4.2",
    "storybook-addon-i18n": "^5.1.13"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "src/index.js",
      "src/storyShots.test.js",
      "src/registerServiceWorker.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/index.js",
      "src/storyShots.test.js",
      "src/registerServiceWorker.js"
    ]
  },
  "scripts": {
    "storybook:start": "npx http-server storybook-static/ -p 3000",
    "postinstall:ui-storybook": "tsc -b tsconfig-build.json && npm run postinstall:copy-svgs",
    "postinstall:copy-svgs": "copyfiles -u 1 src/components/**/*.svg dist",
    "storybook:test": "react-scripts test",
    "storybook:eject": "react-scripts eject",
    "storybook:lint": "tslint --project tslint.json --format codeFrame",
    "storybook:dev": "NODE_OPTIONS=--max_old_space_size=4096 start-storybook -p 9009 -c .storybook watch-css -s public",
    "storybook:build": "NODE_OPTIONS=--max_old_space_size=4096 yarn build-storybook -s public",
    "storybook:snapshot": "build-storybook -s public && percy-storybook --widths=320,1280"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "package.json",
    "README.md",
    "src",
    "tsconfig.json",
    "yarn.lock"
  ],
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@percy/storybook": "3.2.0",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-info": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-links": "5.3.12",
    "@storybook/addon-options": "5.3.12",
    "@storybook/addon-storyshots": "5.3.12",
    "@storybook/addon-viewport": "5.3.12",
    "@storybook/addons": "5.3.12",
    "@storybook/react": "5.3.12",
    "@types/cleave.js": "^1.4.4",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash": "4.14.178",
    "@types/node": "12.7.2",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.8.6",
    "@types/react-transition-group": "^4.4.4",
    "@types/storybook__addon-info": "5.2.1",
    "@types/styled-components": "^5.1.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jest-enzyme": "7.1.2",
    "jest-styled-components": "^7.0.3",
    "react-docgen-typescript-loader": "3.6.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-scripts": "3.3.1",
    "react-test-renderer": "16.8.6",
    "require-context.macro": "1.2.2",
    "resolve-url-loader": "3.1.1",
    "storybook-addon-jsx": "7.1.13",
    "ts-loader": "6.2.0",
    "tslint": "5.14.0",
    "typescript": "3.8.3"
  }
}
