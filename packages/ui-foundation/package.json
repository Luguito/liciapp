{
  "name": "@addi-ui/foundation",
  "version": "1.4.0",
  "devDependencies": {
    "lint-staged": "^11.1.2",
    "style-dictionary": "3.0.1",
    "typescript": "3.8.3"
  },
  "exports": {
    "./": "./"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "scripts": {
    "husky:install": "cd ../../../../ && husky install ./ui/src/apps/ui-foundation/.husky",
    "husky:commit-msg": "husky add .husky/commit-msg \"cd ./ui/src/apps/ui-foundation && npx lint-staged\"",
    "husky:prepare": "rm -rf .husky && yarn husky:install && yarn husky:commit-msg",
    "build:tokens": "node config.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
